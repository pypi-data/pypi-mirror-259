select u.*
from {{dataset_work}}.{{omop_table}}__{{concept_id_column}}_usagi u
inner join {{dataset_omop}}.concept c on c.concept_id = cast(u.conceptId as integer)
  and c.concept_id <> 0 
  and lower(c.domain_id) not in (
  {%- for domain in domains -%}
    {%- if not loop.first -%}
        {{', '}}
    {%- endif -%}
    '{{domain}}'
  {%- endfor -%})
limit 100