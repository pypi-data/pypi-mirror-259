{
    "environment": "ds-composer-v2-airflow-2-dev",
    "log_level": "warning",
    "bucket_name": "us-east1-ds-composer-v2-air-590e040b-bucket",
    "cluster_bucket": "ds-il-dataproc-data",
    "project_id": "dozi-data-science-research-1",
    "region": "us-east1",
    "template_id": "{name-your-service}-template",
    "template_labels": ["{name-your-service}"],
    "project_name": "{name-your-service}",
    "target": "wf_dev",
    "unique_iteration_id": "",
    "unique_template_id": "",
    "specific_jobs_to_run": [],
    "force_remove_all_prerequisites": false,
    "cluster_duration_sec": 3600,
    "jupyter_support": true,
    "cluster_conf": {
        "managed_cluster": {
            "cluster_name": "{name-your-service}-cluster",
            "config": {
                "gce_cluster_config": {
                    "subnetwork_uri": "default",
                    "zone_uri": "us-east1-b",
                    "service_account": "",
                    "metadata": {
                        "PIP_PACKAGES": ""
                    },
                    "tags": [
                        "{name-your-service}-cluster"
                    ],
                    "service_account_scopes":[
                        "https://www.googleapis.com/auth/cloud-platform"
                    ]
                },
                "initialization_actions": [
                    {"executable_file": "gs://dataproc-initialization-actions/python/pip-install.sh"}
                ],
                "master_config": {
                    "machine_type_uri": "n2-standard-2",
                    "disk_config": {
                        "boot_disk_type": "pd-ssd",
                        "boot_disk_size_gb": 40
                    }
                },
                "worker_config": {
                    "machine_type_uri": "n2-standard-2",
                    "num_instances": 3,
                    "disk_config": {
                        "boot_disk_type": "pd-ssd",
                        "boot_disk_size_gb": 40
                    }
                },
                "autoscaling_config": {
                    "policy_uri": ""
                },
                "software_config": {
                    "image_version": "",
                    "optional_components": []
                }
            }
        }
    },
    "spark_properties": [
	    ["spark:spark.sql.execution.arrow.maxRecordsPerBatch", "10"],
	    ["spark:spark.driver.memory", "48"],
	    ["spark:spark.executor.memory", "8"],
	    ["spark:spark.executor.memoryOverhead", "8"],
	    ["spark:spark.sql.shuffle.partitions", "2000"],
	    ["spark.sql.parquet.columnarReaderBatchSize","1024"],
	    ["spark.hadoop.fs.gs.implicit.dir.repair.enable", "false"]
    ],
    "extra_job_params": {
        "MODE": "MANUAL"
    }
}
