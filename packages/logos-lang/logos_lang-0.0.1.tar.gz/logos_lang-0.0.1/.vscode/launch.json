  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File with Arguments",
      "type": "debugpy",
      "request": "launch",
      "program": "main.py",
      "console": "integratedTerminal",
      "args": "${command:pickArgs}"
    },
    {
      "name": "Python: 3.l",
      "type": "python",
      "request": "launch",
      "program": "main.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      // Add argument
      "args": [
        "./programs/3.l"
      ]
    }
  ]
}