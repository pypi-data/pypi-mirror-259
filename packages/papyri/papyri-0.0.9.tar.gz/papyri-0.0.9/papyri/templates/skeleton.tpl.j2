{% include 'head.tpl.j2' %}
<body class="tex2jax_ignore">
    <header>
        <brand><a href='/'><img src='/static/favicon.ico'/></a></brand>
        <nav> / <a href='/' >p</a>  &nbsp;/&nbsp; 
                <div class="dropdown">
                <a href={{module}}>{{module}}</a>&nbsp;/&nbsp;
                    <div class="dropdown-content">
                            {% for url,name in parts_mods -%}
                                <a href="{{url}}">{{name}}</a>
                            {% endfor %}
                     </div>
                </div>
                <div class="dropdown">
                <a href='/p/{{module}}/{{version}}/api/{{module}}{{dothtml}}'>{{version}}</a>&nbsp;/&nbsp;
                    <div class="dropdown-content">
                            {% for v in [version] -%}
                                <a href="/p/{{module}}/{{v}}/api/{{module}}{{dothtml}}">{{v}}</a>
                            {% endfor %}
                    </div>
                </div>

                <div class="dropdown">
                    <a href='/p/{{module}}/{{version}}/api/{{module}}{{dothtml}}'>{{current_type}}</a>&nbsp;/&nbsp;
                    <div class="dropdown-content">
                        <a href="/p/{{module}}/{{version}}/api/{{module}}{{dothtml}}">API</a>
                        <a href="/p/{{module}}/{{version}}/gallery">Gallery</a>
                        <a href="/p/{{module}}/{{version}}/examples">Examples</a>
                        <a href="/p/{{module}}/{{version}}/docs">Narrative</a>
                     </div>
                </div>

            {% for p,links in parts %}
                {% if links %}
                    <div class="dropdown">
                        {% if p =='+' %}
                            <a class="dropbtn" >{{p}}</a>{% if not loop.last %}.{% endif %}
                        {% else %}
                            <a class="dropbtn" href='{{parts_links[p]}}{{dothtml}}'>{{p}}</a>{% if not loop.last %}.{% endif %}
                        {% endif %}
                        {% if not loop.first %}
                            <div class="dropdown-content">
                            {% for url,name in links -%}
                                <a href="{{url}}">{{name}}</a>
                            {% endfor %}
                            </div>
                        {% endif %}
                    </div><!--end dropdown-->
                {% endif -%}
            {% endfor -%}
        </nav>
    </header>
    <div class='container'>

	{% if sidebar %}
    <div class="sidenav">
    {% if not logo %}
        <i class="fab fa-python"></i>
        <a href="#">Project Logo ^</a>
    {% else %}
        <img src="/p/{{module}}/{{version}}/img/{{logo}}"/>
    {% endif %}
    <a href="#">{{module}}</a>
    <a href="#">{{version}}</a>
    {% if meta and meta.get('pypi') %}
        <a class='external'  href="https://pypi.org/project/{{meta.pypi}}">Pypi</a>
    {% endif %}
    {% if meta and meta.get('github_slug') %}
        <a class='external' href="https://github.com/{{meta.github_slug}}">GitHub</a>
    {% endif %}
    {% if meta and meta.get('homepage') %}
        <a class='external' href="{{meta.homepage}}">Homepage</a>
    {% endif %}
    <hr/>
    {% if meta and meta.get('docspage') %}
        <a class='external' href="{{meta.docspage}}">Other Docs</a>
    {% endif %}
    <hr/>
    {% macro tc(obj) -%}
    {%if obj.children%}
        {% if obj.open %}
        <details class='toctree' open>
        {% else %}
        <details class='toctree'>
        {% endif %}
        <summary><a class="toctree {{obj.current}}" href="{{url(obj.ref)}}">{{obj.title}}</a></summary>
            <ul>
            {% for c in obj.children %}
                <li>{{tc(c)}}</li>
            {% endfor %}
            </ul>
        </details>
    {% else %}
        <a class="toctree {{obj.current}}" href="{{url(obj.ref)}}">{{obj.title}}</a>
    {%endif%}
{% endmacro %}
    {% block toctree %}
        {% if toctrees  is defined %}
        <ul>
          {% for toctree in  toctrees%}
               <li> {{tc(toctree)}}</li>

          {% endfor %}
        </ul>
        <hr/>
        {% endif %}
    {% endblock  %}

    {% block plus %}
    {% endblock %}
    </div><!--end sidenav-->
    {% endif%}

    {% block api %}
    {% endblock api %}

</div>
</body>
</html>
